{
  "name": "inventory-management-system",
  "version": "1.0.0",
  "description": "A web-based inventory management system built with TypeScript and React",
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=2048' vite",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' tsc && NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=2048' tsc --noEmit && NODE_OPTIONS='--max-old-space-size=2048' vite build --minify esbuild",
    "build:minimal": "NODE_OPTIONS='--max-old-space-size=1024' vite build --mode development --minify false",
    "build:debug": "NODE_OPTIONS='--max-old-space-size=4096' tsc && NODE_OPTIONS='--max-old-space-size=4096' vite build --verbose",
    "start": "npm run dev",
    "preview": "vite preview",
    "server": "NODE_OPTIONS='--max-old-space-size=2048' ts-node src/server/index.ts",
    "server:js": "NODE_OPTIONS='--max-old-space-size=2048' node src/server/server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.20",
    "@mui/material": "^5.14.20",
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/inquirer": "^9.0.8",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "inquirer": "^12.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^6.3.5"
  }
}
